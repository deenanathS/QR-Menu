<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite  name="Suite" >
  <test thread-count="3" name="Test" parallel="methods">
  <!--Value should be passed as "chrome" to trigger execution in chrome browser and "edge" for internet explorer. Values should be 
  passed as it is as mentioned in previous line-->
   <classes>
       <class name="com.qa.tests.BholaPhonesTest">
       <methods>
        <parameter name="browser1" value="chrome" />
       <include name="signInPositive"></include>
       <parameter name="browser2" value="edge" />
       <include name="signInNegative"></include>
       </methods>
       </class>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
